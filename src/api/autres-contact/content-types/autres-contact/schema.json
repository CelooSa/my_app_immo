{
  "kind": "collectionType",
  "collectionName": "autres_contacts",
  "info": {
    "singularName": "autres-contact",
    "pluralName": "autres-contacts",
    "displayName": "Autres Contacts",
    "description": "Annuaire professionnel par catégories de métiers"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nom": {
      "type": "string",
      "maxLength": 100
    },
    "prenom": {
      "type": "string",
      "maxLength": 100
    },
    "entreprise": {
      "type": "string",
      "maxLength": 150
    },
    "telephone_fixe": {
      "type": "string",
      "maxLength": 20
    },
    "telephone_mobile": {
      "type": "string",
      "maxLength": 20
    },
    "email": {
      "type": "email"
    },
    "adresse": {
      "type": "text"
    },
    "ville": {
      "type": "string",
      "maxLength": 100
    },
    "code_postal": {
      "type": "string",
      "maxLength": 10
    },
    "categorie_principale": {
      "type": "enumeration",
      "enum": [
        "Entrepreneurs",
        "Plombiers",
        "Electriciens",
        "Assureurs",
        "Comptables",
        "Notaires",
        "Architectes",
        "Avocats",
        "Syndics",
        "Agents immobiliers",
        "Diagnostiqueurs",
        "Autres"
      ]
    },
    "sous_categorie": {
      "type": "string",
      "maxLength": 100
    },
    "specialites": {
      "type": "json"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag-contact.tag-contact",
      "mappedBy": "contacts"
    },
    "notes": {
      "type": "text",
      "required": false
    },
    "recommande_par": {
      "type": "string",
      "required": false,
      "maxLength": 100
    },
    "date_ajout": {
      "type": "datetime",
      "default": "now"
    },
    "derniere_utilisation": {
      "type": "date",
      "required": false
    },
    "evaluation": {
      "type": "integer",
      "required": false,
      "min": 1,
      "max": 5
    },
    "actif": {
      "type": "boolean",
      "default": true
    },
    "site_web": {
      "type": "string"
    },
    "siret": {
      "type": "string",
      "maxLength": 14
    },
    "horaires": {
      "type": "text"
    },
    "tarifs_indicatifs": {
      "type": "text"
    },
    "documents": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}